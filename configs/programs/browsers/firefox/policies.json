{
    "policies": {
        "AppAutoUpdate": false,
        "AutofillAddressEnabled": false,
        "AutofillCreditCardEnabled": false,
        "BackgroundAppUpdate": false,
        "Bookmarks": [
            {
                "Title": "shitbox",
                "URL": "https://github.com/ingenarel/shitbox"
            },
            {
                "Title": "gitlab",
                "URL": "https://gitlab.com"
            },
            {
                "Title": "youtube",
                "URL": "https://www.youtube.com"
            },
            {
                "Title": "home manager options",
                "URL": "https://home-manager-options.extranix.com"
            },
            {
                "Title": "policies",
                "URL": "https://mozilla.github.io/policy-templates"
            },
            {
                "Title": "gentoo devmanual",
                "URL": "https://devmanual.gentoo.org/"
            },
            {
                "Title": "guru",
                "URL": "https://github.com/gentoo/guru"
            },
            {
                "Title": "weechat scripts",
                "URL": "https://weechat.org/files/doc/weechat/stable/weechat_scripting.en.html"
            },
            {
                "Title": "PMS gentoo",
                "URL": "https://projects.gentoo.org/pms/8/pms.html"
            }
        ],
        "Cookies": {
            "Allow": [
                "https://archive.org",
                "https://archlinux.org",
                "https://aternos.org",
                "https://chatgpt.com",
                "https://drive.google.com",
                "https://gentoo.org",
                "https://github.com",
                "https://gitlab.com",
                "https://gitlab.com",
                "https://itch.io",
                "https://mail.google.com",
                "https://nixos.org",
                "https://typecraft.dev",
                "https://www.instagram.com",
                "https://www.nexusmods.com",
                "https://www.patreon.com",
                "https://www.sheepit-renderfarm.com",
                "https://www.twitch.tv",
                "https://www.youtube.com",
                "https://x.com"
            ],
            "Behavior": "reject-tracker-and-partition-foreign",
            "BehaviorPrivateBrowsing": "reject-tracker-and-partition-foreign",
            "Locked": true
        },
        "DisableAppUpdate": true,
        "DisableFirefoxAccounts": true,
        "DisableFirefoxStudies": true,
        "DisableFormHistory": true,
        "DisablePocket": true,
        "DisableTelemetry": true,
        "DontCheckDefaultBrowser": true,
        "EnableTrackingProtection": {
            "Cryptomining": true,
            "EmailTracking": true,
            "Fingerprinting": true,
            "Locked": true,
            "Value": true
        },
        "ExtensionUpdate": true,
        "Extensions": {
            "Install": [
                "https://addons.mozilla.org/firefox/downloads/latest/adnauseam/latest.xpi",
                "https://addons.mozilla.org/firefox/downloads/latest/internet_archive_downloader/latest.xpi",
                "https://addons.mozilla.org/firefox/downloads/latest/keepassxc-browser/latest.xpi",
                "https://addons.mozilla.org/firefox/downloads/latest/proton-vpn-firefox-extension/latest.xpi",
                "https://addons.mozilla.org/firefox/downloads/latest/return-youtube-dislikes/latest.xpi",
                "https://addons.mozilla.org/firefox/downloads/latest/sponsorblock/latest.xpi",
                "https://addons.mozilla.org/firefox/downloads/latest/tridactyl-vim/latest.xpi"
            ]
        },
        "NoDefaultBookmarks": false,
        "OfferToSaveLogins": false,
        "SanitizeOnShutdown": true,
        "SearchEngines": {
            "Add": [
                {
                    "Alias": "@ddg",
                    "IconURL": "https://icons.duckduckgo.com/ip3/duckduckgo.com.ico",
                    "Name": "DuckDuckGo",
                    "URLTemplate": "https://duckduckgo.com/?q={searchTerms}"
                },
                {
                    "Alias": "@wp",
                    "IconURL": "https://en.wikipedia.org/static/favicon/wikipedia.ico",
                    "Name": "Wikipedia (en)",
                    "URLTemplate": "https://en.wikipedia.org/w/index.php?search={searchTerms}"
                },
                {
                    "Alias": "@nw",
                    "IconURL": "https://wiki.nixos.org/favicon.ico",
                    "Name": "nixwiki",
                    "URLTemplate": "https://wiki.nixos.org/w/index.php?search={searchTerms}"
                },
                {
                    "Alias": "@np",
                    "IconURL": "https://search.nixos.org/favicon.ico",
                    "Name": "NixPackages",
                    "URLTemplate": "https://search.nixos.org/packages?channel=unstable&type=packages&query={searchTerms}"
                },
                {
                    "Alias": "@no",
                    "IconURL": "https://search.nixos.org/favicon.ico",
                    "Name": "NixOptions",
                    "URLTemplate": "https://search.nixos.org/options?channel=unstable&type=packages&query={searchTerms}"
                },
                {
                    "Alias": "@gw",
                    "IconURL": "https://wiki.gentoo.org/favicon.ico",
                    "Name": "GentooWiki",
                    "URLTemplate": "https://wiki.gentoo.org/index.php?&search={searchTerms}"
                },
                {
                    "Alias": "@gb",
                    "IconURL": "https://bugs.gentoo.org/favicon.ico",
                    "Name": "GentooBugzilla",
                    "URLTemplate": "https://bugs.gentoo.org/buglist.cgi?quicksearch={searchTerms}"
                },
                {
                    "Alias": "@gp",
                    "IconURL": "https://packages.gentoo.org/favicon.ico",
                    "Name": "GentooPackages",
                    "URLTemplate": "https://packages.gentoo.org/packages/search?q={searchTerms}"
                },
                {
                    "Alias": "@gf",
                    "IconURL": "https://github.com/favicon.ico",
                    "Name": "GuruFiles",
                    "URLTemplate": "https://github.com/search?q=repo%3Agentoo%2Fguru+path%3A{searchTerms}&type=code"
                },
                {
                    "Alias": "@gap",
                    "IconURL": "https://gpo.zugaina.org/favicon.ico",
                    "Name": "zugaina",
                    "URLTemplate": "https://gpo.zugaina.org/Search?search={searchTerms}"
                },
                {
                    "Alias": "@aw",
                    "IconURL": "https://wiki.archlinux.org/favicon.ico",
                    "Name": "ArchWiki",
                    "URLTemplate": "https://wiki.archlinux.org/index.php?search={searchTerms}"
                },
                {
                    "Alias": "@ap",
                    "IconURL": "https://archlinux.org/favicon.ico",
                    "Name": "ArchPackages",
                    "URLTemplate": "https://archlinux.org/packages/?&q={searchTerms}"
                },
                {
                    "Alias": "@au",
                    "IconURL": "https://aur.archlinux.org/favicon.ico",
                    "Name": "AUR packages",
                    "URLTemplate": "https://aur.archlinux.org/packages?K={searchTerms}"
                },
                {
                    "Alias": "@gh",
                    "IconURL": "https://github.com/favicon.ico",
                    "Name": "github",
                    "URLTemplate": "https://github.com/search?q={searchTerms}"
                },
                {
                    "Alias": "@fa",
                    "IconURL": "https://addons.mozilla.org/favicon.ico",
                    "Name": "FirefoxAddons",
                    "URLTemplate": "https://addons.mozilla.org/en-US/firefox/search/?q={searchTerms}"
                },
                {
                    "Alias": "@mm",
                    "IconURL": "https://modrinth.com/favicon.ico",
                    "Name": "ModrinthMods",
                    "URLTemplate": "https://modrinth.com/mods?q={searchTerms}"
                },
                {
                    "Alias": "@sp",
                    "IconURL": "https://www.startpage.com/favicon.ico",
                    "Name": "StartPage",
                    "SuggestURLTemplate": "https://www.startpage.com/osuggestions?q={searchTerms}",
                    "URLTemplate": "https://www.startpage.com/sp/search?query={searchTerms}"
                }
            ],
            "Default": "DuckDuckGo",
            "PreventInstalls": true
        }
    }
}
